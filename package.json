{
  "name": "formfish",
  "description": "React Context Form Engine",
  "version": "0.3.1",
  "author": "uStudio Front-End Department",
  "repository": "github:uStudioCompany/formfish",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "typescript"
  ],
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "parcel src/test/index.html --no-cache",
    "clean": "rimraf lib",
    "build:copy": "copyfiles README.md LICENSE package.json lib",
    "build:components": "babel src --out-dir lib --extensions .ts,.tsx --copy-files",
    "build:dts": "tsc -p tsconfig.json",
    "build:clean": "rimraf lib/test",
    "build": "npm-run-all clean -p build:components build:dts build:copy -s build:clean",
    "release": "standard-version",
    "publication": "yarn build && yarn publish lib"
  },
  "dependencies": {
    "copyfiles": "^2.2.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.set": "^4.3.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/yup": "^0.26.30",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
