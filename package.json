{
  "name": "form-engine",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "parcel index.html -p 1488 --no-cache",
    "clean": "rimraf lib",
    "build:components": "babel src --out-dir lib --extensions .ts,.tsx --copy-files",
    "build:dts": "tsc -p tsconfig.json",
    "build:package-json": "copyfiles package.json lib",
    "build": "npm-run-all clean -p build:components build:dts build:package-json"
  },
  "dependencies": {
    "copyfiles": "^2.2.0",
    "lodash.get": "^4.4.2",
