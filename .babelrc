{
  "presets": ["@babel/env", "@babel/react", "@babel/typescript"],
  "plugins": [
    [
      "styled-components",
      {
        "displayName": true,
        "ssr": true
      }
    ],
    "@babel/proposal-nullish-coalescing-operator",
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-optional-chaining"
  ],
  "ignore": ["src/**/*.d.ts"]
}
